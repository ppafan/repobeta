<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <!--<referenceBlock name="shipping_address" remove="true"/>-->
        <referenceBlock name="data" template="Acommerce_Address::order/create/data.phtml">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Acommerce_Address::order/create/data.phtml</argument>
            </action>
            <block class="Acommerce\Address\Block\Adminhtml\Order\Create\Shipping" template="Magento_Sales::order/create/form/address.phtml" name="shipping_address_custom"/>
            <block class="Acommerce\Address\Block\Adminhtml\Order\Create\Billing" template="Magento_Sales::order/create/form/address.phtml" name="billing_address_custom"/>
        </referenceBlock>
    </body>
</page>